<section id="skills" class="py-section bg-secondary">
  <div class="max-w-[1200px] mx-auto px-4 sm:px-6 lg:px-8">
    <h2
      class="font-heading font-semibold text-3xl md:text-4xl mb-12 text-primary"
    >
      Stack & Expertises
    </h2>

    <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-8">
      {
        Object.entries(skillsByDomain).map(([domain, skills]) => (
          <div>
            <h3 class="font-heading font-semibold text-xl mb-4 text-primary capitalize">
              {domain}
            </h3>
            <ul class="space-y-3">
              {skills.map((skill: { label: string; icon: string }) => {
                const hasSvgIcon = skill.icon && skill.icon.trim().startsWith('<svg');
                const questionMarkIcon = `<svg xmlns="http://www.w3.org/2000/svg" width="1.5em" height="1.5em" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"><circle cx="12" cy="12" r="10"/><path d="M9.09 9a3 3 0 0 1 5.83 1c0 2-3 3-3 3"/><path d="M12 17h.01"/></svg>`;
                
                return (
                  <li class="flex items-center gap-3 text-secondary group">
                    <div class="shrink-0 w-6 h-6 flex items-center justify-center text-secondary/60 group-hover:text-accent transition-colors">
                      {hasSvgIcon ? (
                        <Fragment set:html={skill.icon} />
                      ) : (
                        <Fragment set:html={questionMarkIcon} />
                      )}
                    </div>
                    <span class="group-hover:text-primary transition-colors">{skill.label}</span>
                  </li>
                );
              })}
            </ul>
          </div>
        ))
      }
    </div>
  </div>
</section>
